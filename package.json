{
  "name": "space-api-graphql",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn load:data && yarn generate",
    "generate": "yarn generate:schema && yarn generate:types",
    "generate:types": "graphql-codegen --config codegen-server.yml",
    "generate:schema": "ts-node scripts/generate-schema.ts",
    "format": "prettier -w '{api,scripts,seeds}/**/*.{ts,tsx,graphql,yml,json}'",
    "load:data": "knex seed:run"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.0",
    "apollo-datasource": "^0.7.2",
    "apollo-server-micro": "^2.19.0",
    "csv-parse": "^4.14.1",
    "datasource-sql": "^1.3.0",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "sqlite3": "^5.0.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
